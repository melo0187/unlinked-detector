{
  "name": "unlinked-detector",
  "version": "0.0.1",
  "description": "Offers the ability to scan a site for broken links",
  "main": "server.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "serve-debug": "node --inspect dist/server.js",
    "test": "jest --coverage --verbose",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/melo0187/unlinked-detector.git"
  },
  "author": "Carmelo Scollo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/melo0187/unlinked-detector/issues"
  },
  "homepage": "https://github.com/melo0187/unlinked-detector#readme",
  "dependencies": {
    "broken-link-checker": "^0.7.8",
    "express": "^4.16.4",
    "express-ws": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-ws": "^3.0.0",
    "@types/jest": "^23.3.10",
    "@types/shelljs": "^0.8.1",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "shelljs": "^0.8.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "engines": {
    "node": "10.x"
  }
}
